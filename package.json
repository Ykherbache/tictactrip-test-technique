{
  "name": "tictactrip-test-technique",
  "version": "1.0.0",
  "description": "Test technique pour Tic Tac Trip",
  "author": "Yacine Kherbache <yacinekherbache@yaci.fr>",
  "license": "AGPL-3.0",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "_________________development_________________": "",
    "dev": "nodemon",
    "lint": "pnpm prettier && pnpm eslint",
    "lint:fix": "pnpm prettier:fix && pnpm eslint:fix",
    "lint:watch": "nodemon --watch src --watch __tests__ --ext ts --exec \"pnpm lint:fix\"",
    "prettier": "prettier --check '{src,__tests__}/**/*.ts'",
    "prettier:fix": "prettier --write './**/*.ts'",
    "eslint": "eslint {src,__tests__}/**/*.ts",
    "eslint:fix": "eslint --fix ./**/*.ts",
    "_________________production_________________": "",
    "build:clean": "pnpm clean && pnpm build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublishOnly": "pnpm clean && pnpm build",
    "_________________implementation_________________": "",
    "clean": "rm -rf dist coverage",
    "test:unit": "jest --max-workers=1 test/unit",
    "test:integ": "jest --config jest.integ.config.ts --verbose --runInBand --detectOpenHandles",
    "coverage:unit": "jest --coverage --max-workers=1 __tests__/unit ",
    "coverage:integ": "jest --config jest.integ.config.ts --verbose --runInBand --detectOpenHandles --coverage",
    "coverage:all": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@gum-tech/flow-ts": "^1.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "inversify": "^7.10.8",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^8.3.2",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.0",
    "@testcontainers/redis": "^11.11.0",
    "@types/express": "^5.0.6",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "3.4.1",
    "supertest": "^7.1.4",
    "swc": "^1.0.11",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.4"
  },
  "packageManager": "pnpm@9.4.0+sha1.9217c800d4ab947a7aee520242a7b70d64fc7638"
}
